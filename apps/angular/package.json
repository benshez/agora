  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "wipe-android-emulator": "cd C:\\Android\\android-sdk\\emulator && emulator -avd Galaxy_Nexus_API_27 -wipe-data",
    "android": "tns run android --bundle",
    "ios": "tns run ios --bundle",
    "clean-platform-android": "tns platform remove android",
    "clean-platform-ios": "tns platform remove ios",
    "add-platform-android": "tns platform add android",
    "add-platform-ios": "tns platform add ios",
    "build.docker": "ng build --prod --aot && docker-compose build && docker-compose up"
