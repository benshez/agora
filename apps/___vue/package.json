{
  "name": "agora",
  "description": "Agora Web Application",
  "version": "1.0.0",
  "author": "Ben van Heerden <benshez@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --env.tnsAction build",
    "build:android": "npm run build -- --env.android --mode production",
    "build:ios": "npm run build -- --env.ios --mode production",
    "build:web": "npm run build -- --env.web --mode production",
    "debug": "webpack --watch --env.tnsAction debug",
    "debug:android": "npm run debug -- --env.android --mode development  --platform native --environment android",
    "debug:ios": "npm run debug -- --env.ios --mode development",
    "debug:web": "webpack-dev-server --inline --progress --config ./build/webpack.dev.config.js --mode development --platform web",
    "watch": "webpack --watch --env.tnsAction run --mode development",
    "watch:android": "npm run watch -- --env.android --mode development",
    "watch:ios": "npm run watch -- --env.ios --mode development",
    "watch:web": "npm run watch -- --env.web --mode development",
    "clean": "rimraf dist",
    "clean-dist-web": "rimraf dist/web",
    "clean-dist-native": "rimraf  dist/native",
    "clean-dist-native-ios": "rimraf  dist/native/development/ios",
    "clean-dist-native-android": "rimraf  dist/native/development/android",
    "clean-node_modules": "rimraf node_modules"
  },
  "dependencies": {
    "nativescript-theme-core": "^1.0.4",
    "nativescript-vue": "^1.3.1",
    "nativescript-accelerometer": "^2.0.1",
    "nativescript-camera": "^4.0.0",
    "nativescript-fresco": "^4.0.0",
    "nativescript-geolocation": "^4.2.3",
    "nativescript-gradient": "^2.0.1",
    "nativescript-intl": "^3.0.0",
    "nativescript-iqkeyboardmanager": "^1.2.0",
    "nativescript-mapbox": "^4.2.0",
    "nativescript-social-share": "^1.5.0",
    "nativescript-ui-autocomplete": "^3.6.0",
    "nativescript-ui-calendar": "^3.5.0",
    "nativescript-ui-chart": "^3.5.0",
    "nativescript-ui-dataform": "^3.5.0",
    "nativescript-ui-gauge": "^3.5.0",
    "nativescript-ui-listview": "^3.5.1",
    "nativescript-ui-sidedrawer": "^4.3.0",
    "tns-core-modules": "~4.2.0",
    "vuex": "^3.0.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-mdc-adapter": "^0.18.2",
    "vee-validate": "^2.1.0-beta.8",
    "i18n-ts": "^1.0.2",
    "lodash": "^4.17.5",
    "axios": "^0.18.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@types/node": "^10.9.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.1.0",
    "lazy": "1.0.11",
    "nativescript-vue-externals": "^0.2.0",
    "nativescript-vue-loader": "^0.1.5",
    "nativescript-vue-target": "^0.1.0",
    "nativescript-vue-template-compiler": "^1.3.1",
    "node-notifier": "^5.2.1",
    "node-sass": "^4.7.2",
    "ns-vue-loader": "^0.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "ts-loader": "^5.0.0",
    "typescript": "^2.8.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue-loader": "^15.4.1",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2",
    "webpack-synchronizable-shell-plugin": "0.0.7",
    "winston-color": "^1.0.0",
    "yargs": "^12.0.1"
  }
}
