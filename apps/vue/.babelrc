{
  "presets": [
    [
      "es2015",
      {
        "modules": false
      }
    ],
    "stage-2"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./app"],
        "alias": {
          "~": "./*",
          "@": "./*",
          "components": "./components/*",
          "underscore": "lodash"
        }
      }
    ]
  ]
}
