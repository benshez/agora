{
  "name": "my-vue-cli-project",
  "version": "1.0.0",
  "description": "A native application built with NativeScript-Vue",
  "author": "Tiago Alves <tralves@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env.tnsAction build",
    "build:android": "npm run build -- --env.android",
    "build:ios": "npm run build -- --env.ios",
    "build:web": "npm run build -- --env.web",
    "debug": "webpack --watch --env.tnsAction debug",
    "debug:android": "npm run debug -- --env.android",
    "debug:ios": "npm run debug -- --env.ios",
    "debug:web": "webpack-dev-server --inline --progress --config webpack.dev.config.js",
    "watch": "webpack --watch --env.tnsAction run",
    "watch:android": "npm run watch -- --env.android",
    "watch:ios": "npm run watch -- --env.ios",
    "watch:web": "npm run watch -- --env.web",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "nativescript-theme-core": "^1.0.4",
    "nativescript-vue": "^1.3.1",
    "nativescript-accelerometer": "^2.0.1",
    "nativescript-camera": "^4.0.0",
    "nativescript-fresco": "^3.0.6",
    "nativescript-geolocation": "^4.2.3",
    "nativescript-gradient": "^2.0.1",
    "nativescript-intl": "^3.0.0",
    "nativescript-iqkeyboardmanager": "^1.2.0",
    "nativescript-mapbox": "^3.3.0",
    "nativescript-social-share": "^1.5.0",
    "nativescript-ui-autocomplete": "^3.6.0",
    "nativescript-ui-calendar": "^3.5.0",
    "nativescript-ui-chart": "^3.5.0",
    "nativescript-ui-dataform": "^3.5.0",
    "nativescript-ui-gauge": "^3.5.0",
    "nativescript-ui-listview": "^3.5.1",
    "nativescript-ui-sidedrawer": "^3.5.1",
    "tns-core-modules": "~3.4.1",
    "vuex": "^3.0.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-mdc-adapter": "^0.18.2",
    "vee-validate": "^2.0.6",
    "i18n-ts": "^1.0.2",
    "lodash": "^4.17.5",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@types/node": "^9.4.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.1.0",
    "nativescript-vue-externals": "^0.1.2",
    "nativescript-vue-loader": "^0.1.5",
    "nativescript-vue-target": "^0.1.0",
    "nativescript-vue-template-compiler": "^1.3.1",
    "node-notifier": "^5.2.1",
    "node-sass": "^4.7.2",
    "ns-vue-loader": "^0.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "webpack-synchronizable-shell-plugin": "0.0.7",
    "winston-color": "^1.0.0",
    "file-loader": "^1.1.4",
    "lazy": "1.0.11",
